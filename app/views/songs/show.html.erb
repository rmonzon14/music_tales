<h1><%= @song.track_name %> by <%= @song.album.artist.name %></h1>

<div>

  <h4>Song Analytics</h4>

  <p><b>Track Name: </b><%= @song.track_name %></p>
  <p><b>Duration (ms): </b><%= @song.duration_ms %></p>
  <p><b>Tempo: </b><%= @song.tempo%></p>
  <p><b>Liveness: </b><%= @song.liveness %></p>
  <p><b>Valence: </b><%= @song.valence %></p>
  <p><b>Instrumentalness: </b><%= @song.instrumentalness %></p>
  <p><b>Acousticness: </b><%= @song.accousticness %></p>
  <p><b>Speechiness: </b><%= @song.speechiness %></p>
  <p><b>Loudness: </b><%= @song.loudness %></p>
  <p><b>Energy: </b><%= @song.energy %></p>
  <p><b>Key: </b><%= @song.key %></p>
  <p><b>Danceability: </b><%= @song.danceability %></p>

  <div>
    <p>Song Youtube Video Analytics</p>

    <iframe width="420" height="315"
      src="<%= @song.uri.sub("watch?v=", "embed/") %>">
    </iframe>

    <div>
      <p>Youtube video information</p>
      <p>Uploaded by: <%= @song.youtube_video.channel.name %></p>
      <p>Title: <%= @song.youtube_video.title %></p>
      <p>Likes: <%= @song.youtube_video.likes %></p>
      <p>Comments: <%= @song.youtube_video.comments %></p>
      <p>Views: <%= @song.youtube_video.views %></p>
      <p>url: <%= @song.youtube_video.url %></p>
      <p>stream: <%= @song.youtube_video.stream %></p>
      <p>is_official: <%= @song.youtube_video.is_official_video %></p>
      <p>description: <%= @song.youtube_video.description %></p>
    </div>
  </div>

</div>
